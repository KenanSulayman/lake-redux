{
  "name": "lake",
  "version": "0.3.0",
  "description": "a very oppinionated make-based build tool for complex and modular web applications that run on NodeJS",
  "bin": {
    "lake": "./bin/lake",
    "lake-init": "./bin/lake-init",
    "lake-add": "./bin/lake-add",
    "lake-list": "./bin/lake-list",
    "create_component_json": "./bin/create_component_json",
    "manifest-migration": "./bin/manifest-migration"
  },
  "repository": {
    "type": "git",
    "url": "http://github.com/regular/lake.git"
  },
  "contributors": [
    "Bastian Albers (http://github.com/bastianalbers)",
    "Anton Wilhelm <timaschew@gmail.com> (http://github.com/timaschew)",
    "Andreas Rotter (http://github.com/andirotter)",
    "Philipp Thürwächter (http://github.com/pithu)",
    "Jan Bölsche <jan@lagomorph.de> (http://github.com/regular)"
  ],
  "scripts": {
    "build": "./compile.sh",
    "postinstall": "./compile.sh",
    "postupdate": "./compile.sh",
    "clean": "rm -rf lib",
    "prepublish": "npm run build",
    "postpublish": "npm run clean",
    "test": "npm run build; cd test; ../bin/lake features/testlmake/integration_test"

  },
  "readmeFilename": "README.md",
  "dependencies": {
    "coffee-script": "~1.6.3",
    "nconf": "~0.6.7",
    "underscore": "~1.4.4",
    "debug": "~0.7.2",
    "mkdirp": "~0.3.5",
    "carrier": "~0.1.10",
    "findup-sync": "~0.1.2",
    "nopt": "~2.1.1",
    "once": "~1.1.1",
    "eco": "~1.1.0-rc-3",
    "async": "~0.2.9",
    "pipette": "~0.9.3",
    "component": "~0.17.2",
    "stylus": "~0.37.0",
    "nib": "~1.0.1",
    "jade": "~0.28.1",
    "express": "~3.3.8",
    "supertest": "~0.7.1",
    "mocha-phantomjs": "~3.1.2",
    "phantom": "~0.5.2",
    "phantomjs": "~1.9.2-0",
    "js2coffee": "~0.2.0"
  },
  "devDependencies": {
    "chai": "1.5.x",
    "mocha": "~1.8.1",
    "difflet": "~0.2.6",
    "dive": "~0.3.0"
  }
}
